#! /bin/zsh 
# Configuration that is user-specific.
# Author: Chris Fichman
# Email: chris.fichman@terazo.com

# User info
export PROFILE=
export PROJECT_NAME=terazo
export USER_EMAIL=

# Github account setup
export GIT_PROFILE=
export SSH_KEYNAME=
export SSH_FILE=~/.ssh/${SSH_KEYNAME}
export SSH_HOSTS="*"

# Python Preference
export USER_PYTHON_VERSION=3.10.5

# Twilio Profile Information for Terazo partner account
# You must have created a twilio profile for this to work!
export TWILIO_PROFILE=${PROJECT_NAME}-dev
export TWILIO_TERAZO_SID=
export TWILIO_TERAZO_KEY=

# Current Terazo Project
export CURRENT_PROJECT=