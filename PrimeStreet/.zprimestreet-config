#! /bin/zsh
# This file contains configurations and that are relevant to development on the PrimeStreet Project
# Author: Chris fichman
# Email: chris.fichman@terazo.com
export PROJECT_NAME=primestreet

# Java Preverence
export PROJECT_JAVA_MAJOR_VERSION=11

# Twilio Profile Information for Terazo partner account
# You must have created a twilio profile for this to work!
export TWILIO_PROFILE=${PROJECT_NAME}-dev
export TWILIO_PRIMESTREET_SID=
export TWILIO_PRIMESTREET_KEY=

# Primestreet github account setup you can use same ssh key or a new one.
export SSH_KEYNAME=
export SSH_FILE=~/.ssh/${SSH_KEYNAME}
export SSH_HOSTS="*PrimeStreet*"